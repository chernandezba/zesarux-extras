       EOR.B   D5,D0
       AND.W   D4,D0
       BNE     L0151C
L012DE ADDQ.W  #8,A4
       CMPA.L  $005C(A6),A4
       BLT.S   L012C6
       MOVEQ   #$00,D0
       RTS

LPOSAD 
