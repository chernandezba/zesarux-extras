       LEA     $0016(A2),A3
       MOVE.L  (A3)+,(A1)+
       MOVE.L  (A3)+,(A1)+
       MOVE.W  (A3)+,(A1)+
       MOVE.W  #$01FC,D0
       MOVEQ   #$00,D1
       MOVEQ   #$01,D2
