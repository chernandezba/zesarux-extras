LBADPAR
XLBADPAR EQU LBADPAR
L012A6 MOVEQ   #-$0F,D0
       RTS

LVERIF 
XLVERIF EQU LVERIF
       MOVEQ   #$03,D4
       MOVEQ   #-$01,D5
       BRA.S   L012B4
       
LTRANS
